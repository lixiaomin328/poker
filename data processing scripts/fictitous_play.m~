%fitting fictitious play
subId = 3;
payoffparams = [3,1,1];
[p1Actions,p1Cards,p1rt,p2Actions,p2Cards,p2rt] = getIndividualRTandChoice(subId);
addpath('Users/lixiaomin/Documents/GitHub/poker/model_fitting');
assessment = [];
for i = 1:length(p1Actions)
    p1mix = sum(p1Actions(1:i)==1)/sum(~p1Actions(1:i)==-1);
    p2mix = sum(p2Actions(1:i)==1)/sum(~p1Actions(1:i)==-1);
    assessment = [assessment;p1mix];
texasLevelDensity(p1mix,p2mix,payoffparams)
end